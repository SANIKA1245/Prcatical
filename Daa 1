//Non - recursive

nterms = int(input("How many terms?"))

n1,n2 = 0, 1
count = 0

if nterms<=0:
    print("Please enter the positive integer")
elif nterms==1:
    print("Fibonacci sequence up to", nterms,":")
    print(n1)
else:
    print("Fibonacci sequence :")
    while count< nterms :
        print(n1)
        nth = n1+ n2

        n1= n2
        n2= nth
        count +=1

//Recursive

def fibonacci(n):
    if n <= 0:
        return[]
    elif n == 1:
        return[0]
    elif n == 2:
        return[0,1]
    else:
        seq = fibonacci(n-1)
        seq.append(seq[-1] + seq[-2])
        return seq

nterms = int(input("How many terms ? :"))

if nterms <= 0:
        print("Please enter the positive integer")
else:
        print("Fibonacci sequence :")
        fib_sequence = fibonacci(nterms)
        for num in fib_sequence:
            print(num)
        
    
